import java.util.ArrayList;

public class RiddleTrials {
	private ArrayList<RiddleScenario> trialListt;
	private Map<Integer, RiddleScenario> trialList;
	private int numOfTrials;
	
	RiddleTrials(RiddleScenario riddleScenario, int numOfTrials) {
		this.numOfTrials = numOfTrials;
		for (int i = 1; i <= numOfTrials; i++) {
			trialList.put(i, riddleScenario);
		}
	}
	
	public void displayTrialResults() {
		
	}
	
	public ArrayList<RiddleScenario> getTrialList() {
		return trialList;
	}
	public int getNumOfTrials() {
		return numOfTrials;
	}
}
